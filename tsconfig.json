{
    "compilerOptions": {
        "outDir": "",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./src",
        "paths": {
            "@store/*": [
                "./store/*"
            ],
            "@api/*": [
                "./api/*"
            ],
            "@components/*": [
                "./components/*"
            ],
            "@type/*": [
                "./type/*"
            ],
        },
    }
}